<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestwebserviceService WSDL service -->

  <service name="Testwebservice" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://radha.com"/>
      <parameter name="wsdlServiceElement" value="TestwebserviceService"/>
      <parameter name="schemaQualified" value="http://radha.com"/>
      <parameter name="wsdlServicePort" value="Testwebservice"/>
      <parameter name="className" value="com.radha.TestwebserviceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Testwebservice"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sayHello" qname="operNS:SayHello" xmlns:operNS="http://radha.com" soapAction="" >
        <parameter qname="pns:message" xmlns:pns="http://radha.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sayHello"/>

  </service>
</deployment>
